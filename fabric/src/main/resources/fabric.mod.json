{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "version": "${version}",
  "name": "${mod_name}",
  "description": "${description}",
  "authors": [
    "${mod_author}"
  ],
  "contact": {
    "repo": "https://github.com/lanAnderson/SeasonHUD",
    "issues": "https://github.com/lanAnderson/SeasonHUD/issues"
  },
  "license": "${license}",
  "icon": "${mod_id}.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "club.iananderson.seasonhud.fabric.client.SeasonHudClientFabric"
    ],
    "main": [
      "club.iananderson.seasonhud.fabric.SeasonHudFabric"
    ]
  },
  "mixins": [
    "seasonhud.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${fabric_loader_version}",
    "fabric-api": "*",
    "minecraft": "${minecraft_version}",
    "java": ">=${java_version}",
    "seasons": ">=2.0",
    "fabric-key-binding-api-v1": "*",
    "forgeconfigapiport": "*"
  },
  "breaks": {
    "xaerominimap": "<24.0.0",
    "map_atlases": "<1.19-6.0.0"
  },
  "suggests": {
    "seasonsextras": "*",
    "ftbchunks": ">=1902.4.2"
  }
}
  