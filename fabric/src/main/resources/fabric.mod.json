{
  "schemaVersion": 1,
  "id": "seasonhud",
  "version": "${version}",
  "name": "SeasonHUD",
  "description": "This is an addon for SereneSeasons that displays the current season on the HUD or under a minimap",
  "authors": [
    "IanAnderson"
  ],
  "contact": {
    "repo": "https://github.com/lanAnderson/SeasonHUD"
  },
  "license": "All-Rights-Reserved",
  "icon": "SeasonHUD.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "club.iananderson.seasonhud.client.SeasonHUDClientFabric"
    ],
    "main": [
      "club.iananderson.seasonhud.SeasonHUD"
    ]
  },
  "mixins": [
    "seasonhud.fabric.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${fabric_loader_version}",
    "fabric-api": "*",
    "minecraft": "${minecraft_version}",
    "java": ">=17",
    "sereneseasons": "*",
    "fabric-key-binding-api-v1": "*",
    "forgeconfigapiport": "*"
  },
  "breaks": {
    "xaerominimap": "<24.0.0",
    "map_atlases": "<2.6.1"
  }
}
  